p01 = Program(source=['CalibrateOsm.cpp'], LIBS=['routingkit'])
p02 = Program(source=['ComputeDijkstraRanks.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p03 = Program(source=['ComputeDistances.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p04 = Program(
    source=['ComputeUnionBoundary.cpp'], parse_flags='-DCSV_IO_NO_THREAD',
    LIBS=['CGAL', 'gmp', 'mpfr', 'proj'])
p05 = Program(
    source=['ConvertGraph.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj', 'routingkit'])
p06 = Program(source=['DrawNetwork.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['cairo'])
p07 = Program(source=['GenerateMatchingODPairs.cpp'], LIBS=['routingkit'])
p08 = Program(source=['GenerateODPairs.cpp'], parse_flags='-DCSV_IO_NO_THREAD -fopenmp')
p09 = Program(source=['ParseMobiToppFile.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p10 = Program(source=['ParseMtxFile.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p11 = Program(source=['RadiationModel.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj'])
p12 = Program(source=['VisumExtractPoi.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj'])

Alias('CalibrateOsm', p01)
Alias('ComputeDijkstraRanks', p02)
Alias('ComputeDistances', p03)
Alias('ComputeUnionBoundary', p04)
Alias('ConvertGraph', p05)
Alias('DrawNetwork', p06)
Alias('GenerateMatchingODPairs', p07)
Alias('GenerateODPairs', p08)
Alias('ParseMobiToppFile', p09)
Alias('ParseMtxFile', p10)
Alias('RadiationModel', p11)
Alias('VisumExtractPoi', p12)

Return('p01', 'p02', 'p03', 'p04', 'p05', 'p06', 'p07', 'p08', 'p09', 'p10', 'p11', 'p12')
